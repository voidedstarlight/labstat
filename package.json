{
	"devDependencies": {
		"@eslint/js": "^9.38.0",
		"@stylistic/eslint-plugin": "^5.5.0",
		"@tsconfig/node24": "^24.0.1",
		"@tsconfig/recommended": "^1.0.10",
		"@types/node": "24.7.2",
		"@typescript/native-preview": "^7.0.0-dev.20251017.1",
		"bufferutil": "^4.0.9",
		"concurrently": "^9.2.1",
		"eslint": "^9.38.0",
		"jiti": "^2.6.1",
		"tsc-watch": "^7.2.0",
		"typescript-eslint": "^8.46.1",
		"utf-8-validate": "^6.0.5"
	},
	"license": "MIT",
	"name": "labstat",
	"scripts": {
		"build:front": "npm run build -w @labstat/web && npm run build -w @labstat/api",
		"build:node": "npm run build -w @labstat/node",
		"dev:front": "concurrently -n 'WEB,API,@UI,@UT' -c 'bgBlue.black,bgMagentaBright.black,bgBlack.white' 'npm run dev -w @labstat/web' 'npm run dev -w @labstat/api' 'npm run dev -w @labstat/ui' 'npm run dev -w @labstat/util'",
		"dev:node": "concurrently -n 'SRV,@UT' -c 'bgCyan.black,bgBlack.white' 'npm run dev -w @labstat/node' 'npm run dev -w @labstat/util'",
		"lint": "eslint apps lib"
	},
	"version": "0.0.1",
	"workspaces": [
		"apps/*",
		"lib/*"
	]
}
